{
  "workbench.iconTheme": "vscode-icons",
  "editor.formatOnSave": false,
  "go.useLanguageServer": false,
  "vsicons.associations.files": [
    {
      "icon": "docker",
      "extensions": ["docker-compose.prod.yml"],
      "filename": true,
      "format": "svg"
    },
    {
      "icon": "dockertest",
      "extensions": ["docker-compose.testing-stage.yml"],
      "filename": true,
      "format": "svg"
    },
    {
      "icon": "config",
      "extensions": ["plist", "properties", "dotenv", "rabbitmq.conf"],
      "filename": true,
      "format": "svg"
    }
  ],
  "vsicons.associations.folders": [
    {
      "icon": "paket",
      "extensions": [".paket", "delivery"],
      "format": "FileFormat.svg",
      "disabled": false
    },
    {
      "icon": "package",
      "extensions": ["package", "packages", "pkg", "storage"],
      "format": "FileFormat.svg",
      "disabled": false
    },
    {
      "icon": "services",
      "extensions": ["services", "context"],
      "format": "FileFormat.svg",
      "disabled": false
    },
    {
      "icon": "controller",
      "extensions": ["controllers", "controller", "handlers", "hooks"],
      "format": "FileFormat.svg",
      "disabled": false
    },
    {
      "icon": "cli",
      "extensions": ["cmd", "command", "commands", "cli"],
      "format": "FileFormat.svg",
      "disabled": false
    },
    {
      "icon": "locale",
      "extensions": ["language", "content"],
      "format": "FileFormat.svg",
      "disabled": false
    },
    {
      "icon": "helper",
      "extensions": ["helper", "common"],
      "format": "FileFormat.svg",
      "disabled": false
    },
    {
      "icon": "view",
      "extensions": ["pages", "page", "features"],
      "format": "FileFormat.svg",
      "disabled": false
    },
    {
      "icon": "test",
      "extensions": ["test", "tests", "api-tests"],
      "format": "FileFormat.svg",
      "disabled": false
    },
    {
      "icon": "story",
      "extensions": ["stories", ".storybook"],
      "format": "FileFormat.svg",
      "disabled": false
    },
    {
      "icon": "route",
      "extensions": ["route", "routes", "channel", "channels"],
      "format": "FileFormat.svg",
      "disabled": false
    },
    {
      "icon": "android",
      "extensions": ["android", ".well-known"],
      "format": "FileFormat.svg",
      "disabled": false
    },
    {
      "icon": "mock",
      "extensions": ["mocks", "mock-api"],
      "format": "FileFormat.svg",
      "disabled": false
    },
    {
      "icon": "www",
      "extensions": ["rabbitmq", "www", "wwwroot"],
      "format": "FileFormat.svg",
      "disabled": false
    },
    {
      "icon": "settings",
      "extensions": ["settings"],
      "format": "FileFormat.svg",
      "disabled": true
    }
  ]
}
